{
  "env": {
    "REACT_APP_BACKEND_URL": "https://gambling-wins-backend.onrender.com"
  },
  "build": {
    "env": {
      "REACT_APP_BACKEND_URL": "https://gambling-wins-backend.onrender.com"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://gambling-wins-backend.onrender.com/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 
